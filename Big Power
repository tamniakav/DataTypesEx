using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Numerics;

namespace P03BigPower
{
    class BigPower
    {
        static void Main(string[] args)
        {
            BigInteger n =BigInteger.Parse(Console.ReadLine());
            BigInteger sum = 1;
            for (int i = 0; i < n; i++)
            {
                sum *= n;
            }
            Console.WriteLine(sum);
        }
    }
}
